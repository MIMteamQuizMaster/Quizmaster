                     <Button fx:id="btnEditQuestion" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#editQuestionBtnAction" text="Bijwerken" />
                     <Button fx:id="btnCancelQuestion" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#cancelQuestionBtnAction" text="annuleren" VBox.vgrow="ALWAYS" />
                  </children>
               </VBox>
            </children>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         </HBox>
         <Label prefHeight="17.0" prefWidth="85.0" text="Antwoord:" />
